{
	"name": "@ctx-core/cognito",
	"version": "10.2.181",
	"description": "@ctx-core aws cognito bindings",
	"keywords": [
		"ctx-core",
		"aws",
		"cognito"
	],
	"homepage": "https://github.com/ctx-core/cognito#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/cognito/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/cognito.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-cognito@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./authenticateUser",
		"./changePassword",
		"./cognito_user_",
		"./confirmRegistration",
		"./deleteAttributes",
		"./deleteUser",
		"./forgotPassword",
		"./getAttributeVerificationCode",
		"./getUserAttributes",
		"./globalSignOut",
		"./Pool",
		"./resendConfirmationCode",
		"./signOut",
		"./signUp",
		"./updateAttributes"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"amazon-cognito-identity-js": "^6.3.12",
		"ctx-core": "^7.2.3"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.9.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}