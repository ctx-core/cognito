{
	"name": "@ctx-core/cognito",
	"version": "8.0.3",
	"description": "@ctx-core aws cognito bindings",
	"keywords": [
		"ctx-core",
		"aws",
		"cognito"
	],
	"homepage": "https://github.com/ctx-core/cognito#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/cognito/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/cognito.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"amazon-cognito-identity-js": "^5.0.3"
	},
	"devDependencies": {
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}